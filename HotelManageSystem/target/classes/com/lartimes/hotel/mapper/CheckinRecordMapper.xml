<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lartimes.hotel.mapper.CheckinRecordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lartimes.hotel.model.po.CheckinRecord">
        <id column="id" property="id" />
        <result column="guest_id" property="guestId" />
        <result column="room_id" property="roomId" />
        <result column="checkin_date" property="checkinDate" />
        <result column="checkout_date" property="checkoutDate" />
        <result column="payment" property="payment" />
        <result column="remark" property="remark" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        guest_id, room_id, checkin_date, checkout_date, payment, remark, id
    </sql>

</mapper>
